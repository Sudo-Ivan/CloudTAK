        "doc": "apidoc -i ./doc",
        "prod": "node index.js",
        "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
        "build": "tsc",
        "dev": "nodemon -i schema/ -i web/ -i doc/ --esm index.ts"
