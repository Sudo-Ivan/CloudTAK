        "@openaddresses/batch-schema": "^7.5.0",
        "@placemarkio/check-geojson": "^0.1.12",
        "@tak-ps/blueprint-login": "^0.2.0",
        "@tak-ps/node-cot": "^2.8.0",
        "@turf/meta": "^6.5.0",
        "@types/cors": "^2.8.13",
        "@types/ws": "^8.5.4",
        "ajv": "^8.11.2",
        "ajv-formats": "^2.1.1",
        "busboy": "^1.6.0",
