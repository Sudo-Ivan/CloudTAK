{
    "name": "@tak-ps/etl.api",
    "version": "1.0.0",
    "type": "module",
    "description": "Facilitate ETL operations to bring non-TAK sources into a TAK Server",
    "main": "index.js",
    "scripts": {
        "test": "./test/test",
        "build": "tsc && rsync -av progress . dist/ --exclude web/ --exclude node_modules/ || true && rm dist/routes/*.ts",
        "prod": "NODE_OPTIONS='--max-old-space-size=6144' ts-node index.ts",
        "lint": "eslint index.ts lib/**.ts test/**.ts",
        "loaddb": "echo 'DROP DATABASE tak_ps_etl' | psql && echo 'CREATE DATABASE tak_ps_etl' | psql && psql tak_ps_etl < coe-etl-latest.dump && knex migrate:latest",
        "dev": "nodemon -i schema/ -i web/ --esm index.ts --noevents --nosinks --nometrics --unsafe"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tak-ps/etl.git"
    },
    "author": "Nick Ingalls <nick@ingalls.ca>",
    "license": "ISC",
