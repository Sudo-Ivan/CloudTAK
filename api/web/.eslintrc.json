{
    "root": true,
    "globals": {},
    "env": {
        "node": true
    },
    "extends": [
        "plugin:vue/essential",
        "eslint:recommended"
    ],
    "parserOptions": {
        "parser": "@babel/eslint-parser"
    },
    "rules": {
        "vue/multi-word-component-names": 1,
        "vue/no-multiple-template-root": 0,
        "vue/no-v-model-argument": 0,
        "vue/require-v-for-key": 0
    }
}
