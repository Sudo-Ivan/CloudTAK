{
    "type": "object",
    "additionalProperties": false,
    "required": [
        "status"
    ],
    "properties": {
        "id": {
            "$ref": "./server/id.json"
        },
        "status": {
            "type": "string",
            "enum": [
                "configured",
                "unconfigured"
            ]
        },
        "created": {
            "$ref": "./server/created.json"
        },
        "updated": {
            "$ref": "./server/updated.json"
        },
        "name": {
            "$ref": "./server/name.json"
        },
        "url": {
            "$ref": "./server/url.json"
        },
        "api": {
            "$ref": "./server/api.json"
        },
        "auth": {
            "type": "boolean",
            "description": "Once an admin certificate is configured it is not retrivable. This boolean refers to if a certificate is currently loaded"
        }
    }
}
