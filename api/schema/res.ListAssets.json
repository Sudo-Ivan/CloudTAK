{
    "type": "object",
    "required": [
        "total",
        "tiles",
        "assets"
    ],
    "additionalProperties": false,
    "properties": {
        "total": {
            "$ref": "./util/total.json"
        },
        "tiles": {
            "type": "object",
            "required": [ "url" ],
            "additionalProperties": false,
            "properties": {
                "url": {
                    "type": "string"
                }
            }
        },
        "assets": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "name",
                "sync",
                "updated",
                "visualized",
                "vectorized",
                "etag",
                "size"
            ],
            "properties": {
                "name": {
                    "type": "string",
                    "description": "The filename of the asset"
                },
                "sync": {
                    "type": "boolean",
                    "description": "If the Data is attached to a mission, signify if the asset is syncing with the mission"
                },
                "visualized": {
                    "type": ["string", "boolean"],
                    "description": "A string containing the path to a Cloud Optimized version of the asset has been generated, or false"
                },
                "vectorized": {
                    "type": ["string", "boolean"],
                    "description": "A string containing the path to a GeoJSONLD Vectorized version of the asset has been generated or false"
                },
                "updated": {
                    "type": "integer",
                    "description": "Timestamp at which the asset was last modified"
                },
                "etag": {
                    "type": "string",
                    "description": "AWS S3 generated ETag of the asset"
                },
                "size": {
                    "type": "integer",
                    "description": "Size in bytes of the asset"
                }
            }
        }
    }
}
